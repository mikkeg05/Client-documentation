@model Client
@{ 
<div class="col-lg-6">
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Version Control & Deployment strategy</h6>
        </div>
        <div class="card-body">
            <p>@Model.VersionControlValue</p>
            <p>@Model.DeploymentStrategy</p>
        </div>
    </div>
</div>
    Html.RenderPartial($"~/Views/Partials/NestedContent/Branching.cshtml", Model.Branching);

}