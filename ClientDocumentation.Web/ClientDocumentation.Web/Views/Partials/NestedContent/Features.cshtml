@model IEnumerable<KeyFeature>
<div class="col-lg-6">
    <div class="card shadow mb-4">
        <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
            <h6 class="m-0 font-weight-bold text-primary">Features</h6>

        </div>
        <div class="card-body">
            @{

                foreach (var item in Model)
                {
                    
                    var indexString = "a"+item.Key.ToString();
                   
                    <a href="#@indexString" class="d-block card-header py-3" data-toggle="collapse"
                       role="button" aria-expanded="true" aria-controls="collapseCardExample">
                        <h6 class="m-0 font-weight-bold text-primary">@item.FeatureName</h6>
                    </a>
                    <div class="collapse" id="@indexString">
                        <div class="card-body">
                           
                            <p>@item.Description</p>
                            <p>Found @item.Section</p>

                        </div>
                    </div>
                }
            }
        </div>
    </div>

</div>